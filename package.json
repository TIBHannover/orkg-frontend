        "enzyme-adapter-react-16": "^1.14.0",
        "enzyme-to-json": "^3.4.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.4.0",
        "eslint-plugin-prettier": "^3.1.1",
        "husky": "^3.0.7",
        "jest-canvas-mock": "^2.1.1",
        "lint-staged": "^9.4.1",
        "named-urls": "^1.4.0",
        "prettier": "^1.18.2",
        "react-app-rewired": "^2.1.6",
        "react-test-renderer": "^16.9.0",
        "sass-extract": "^2.1.0",
        "sass-extract-js": "^0.4.0",
        "sass-extract-loader": "^1.1.0"
    },
    "auto-changelog": {
        "template": "./keepachangelog.hbs",
        "unreleased": false,
        "commitLimit": 0,
        "tagPattern": ".+",
        "tagPrefix": "V"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "npm run --silent lint:fix",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 9",
        "not op_mini all"
    ],
    "babelMacros": {
        "styledComponents": {
            "pure": true
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "prettier"
        ],
        "plugins": [
            "prettier"
        ],
        "rules": {
            "prettier/prettier": [
                "error"
            ],
            "react/jsx-closing-bracket-location": [
                "error"
            ],
            "react/jsx-closing-tag-location": [
                "error"
            ],
            "react/sort-comp": [
                "error"
            ],
            "jsx-quotes": [
                "error"
            ],
            "no-multi-spaces": [
                "error"
            ],
            "react/jsx-tag-spacing": [
                "error"
            ],
            "react/jsx-curly-spacing": [
                "error"
            ],
            "curly": [
                "error"
            ],
            "react/jsx-wrap-multilines": [
                "error"
            ],
            "react/self-closing-comp": [
                "error"
            ],
            "react/prop-types": [
                "error"
            ],
            "no-var": [
                "error"
            ],
            "no-const-assign": [
                "error"
            ],
            "prefer-const": [
                1
            ],
            "react/jsx-curly-brace-presence": [
                "error"
            ]
        }
    }
}
