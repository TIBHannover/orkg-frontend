    "auto-changelog": {
        "template": "./keepachangelog.hbs",
        "unreleased": false,
        "commitLimit": 0,
        "tagPattern": ".+",
        "tagPrefix": "V"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx}": [
            "eslint --fix --quiet"
        ],
        "src/**/*.{ts,tsx}": [
            "bash -c tsc -p tsconfig.json --noEmit"
        ]
