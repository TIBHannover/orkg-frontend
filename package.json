    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "prestart": "react-env --env APP_ENV",
        "start": "craco start",
        "build": "react-env && DISABLE_ESLINT_PLUGIN=true craco build",
        "test": "craco test --env=jsdom",
        "test:ci": "craco test --ci --env=jsdom --reporters=default --reporters=jest-junit --runInBand",
        "flow": "flow",
        "lint": "eslint src",
        "lint:fix": "eslint src --fix",
        "lint:quiet": "eslint src --quiet",
        "type-check": "tsc --noEmit",
        "postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\"",
        "prepare": "husky install",
        "storybook": "APP_ENV= react-env --env APP_ENV && storybook dev -p 6006",
        "build-storybook": "APP_ENV= react-env --env APP_ENV && storybook build --output-dir public/storybook"
    },
