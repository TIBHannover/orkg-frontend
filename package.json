    "scripts": {
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "prestart": "react-env",
        "start": "react-scripts start",
        "build": "react-env && react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "test:ci": "react-scripts test --ci --env=jsdom --reporters=default --reporters=jest-junit --runInBand",
        "eject": "react-scripts eject",
        "flow": "flow",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "version": "auto-changelog -t keepachangelog.hbs && git add CHANGELOG.md",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.16.11",
        "@beam-australia/react-env": "^3.1.1",
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "auto-changelog": "^2.4.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^3.3.1",
        "faker": "^5.5.3",
        "husky": "^7.0.4",
        "jest-canvas-mock": "^2.3.1",
        "jest-junit": "^13.2.0",
        "lint-staged": "^12.4.0",
        "msw": "^0.39.2",
        "prettier": "1.18.2",
        "react-scripts": "5.0.1",
        "react-select-event": "^5.5.0",
        "sass": "^1.51.0"
    },
    "jest-junit": {
        "classNameTemplate": "{classname}",
        "titleTemplate": "{title}",
        "addFileAttribute": "true"
    },
    "auto-changelog": {
        "template": "./keepachangetestlog.hbs",
        "unreleased": false,
        "commitLimit": 0,
        "tagPattern": ".+",
        "tagPrefix": "V"
    },
    "lint-staged": {
        "src/**/*.{js,jsx}": [
            "npm run --silent lint:fix"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie < 9",
        "not op_mini all"
    ],
    "babelMacros": {
        "styledComponents": {
            "pure": true
        }
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "plugin:prettier/recommended"
        ],
        "plugins": [
            "prettier",
            "jsx-a11y"
        ],
        "rules": {
            "prettier/prettier": [
                "error"
            ],
            "react/jsx-closing-bracket-location": [
                "error"
            ],
            "react/jsx-closing-tag-location": [
                "error"
            ],
            "react/sort-comp": [
                "error"
            ],
            "jsx-quotes": [
                "error"
            ],
            "no-multi-spaces": [
                "error"
            ],
            "react/jsx-tag-spacing": [
                "error"
            ],
            "react/jsx-curly-spacing": [
                "error"
            ],
            "curly": [
                "error"
            ],
            "react/jsx-wrap-multilines": [
                "error"
            ],
            "react/self-closing-comp": [
                "error"
            ],
            "react/prop-types": [
                "error"
            ],
            "no-var": [
                "error"
            ],
            "no-const-assign": [
                "error"
            ],
            "prefer-const": [
                1
            ],
            "react/jsx-curly-brace-presence": [
                "error"
            ],
            "jsx-a11y/no-autofocus": [
                2,
                {
                    "ignoreNonDOM": true
                }
            ],
            "jsx-a11y/click-events-have-key-events": [
                "warn"
            ],
            "jsx-a11y/no-static-element-interactions": [
                "warn"
            ],
            "jsx-a11y/no-noninteractive-element-interactions": [
                "warn"
            ]
        }
    }
}
