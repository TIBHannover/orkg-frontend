        "flow": "flow",
        "lint": "eslint 'src/**/*.{js,jsx}'",
        "lint:fix": "eslint 'src/**/*.{js,jsx}' --fix",
        "version": "auto-changelog && git add CHANGELOG.md"
    },
    "devDependencies": {
        "auto-changelog": "^1.16.2",
        "babel-preset-env": "^1.7.0",
        "customize-cra": "^1.0.0",
        "customize-cra-react-refresh": "^1.1.0",
